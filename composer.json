{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^8.4",
    "behat/behat": "*",
    "symfony/messenger": "^7.3",
    "symfony/console": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "doctrine/orm": "^3.5",
    "doctrine/doctrine-bundle": "^3.0",
    "doctrine/migrations": "^3.9",
    "doctrine/doctrine-migrations-bundle": "^3.5",
    "symfony/dotenv": "^7.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.4",
    "phpstan/phpstan": "^1.12",
    "friendsofphp/php-cs-fixer": "^3.20",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "allow-plugins": {
      "ocramius/package-versions": true,
      "symfony/flex": true
    }
  },
  "autoload": {
    "psr-4": {
      "Fulll\\": "src/"
    }
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse -c phpstan.neon",
    "cs-fix": "vendor/bin/php-cs-fixer fix --allow-risky=yes --config=.php-cs-fixer.dist.php",
    "cs-check": "vendor/bin/phpcs --standard=ruleset.xml src tests",
    "ci:lint": "composer analyse && vendor/bin/php-cs-fixer fix --dry-run --diff --config=.php-cs-fixer.dist.php && vendor/bin/phpcs --standard=ruleset.xml src tests",
    "test": "vendor/bin/phpunit --configuration phpunit.xml"
  }
}

